@model CRM.Areas.JJD.Models.BankManagerComplexModel

@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <title></title>
    <link href="~/Areas/JJD/Content/css/mui.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Areas/JJD/Content/css/05.bankManager/index.css" />
</head>

<body>
    <div id="check" style="display: none" class="a4"></div>
    <div class='heads'>
        <img id="Face" src="~/Areas/JJD/Content/images/01.index/touxiang.png" />
        <a id="Name"></a>
        <h5>@Model.User.UserName</h5>
        <h6>信贷经理</h6>
    </div>
    <div class='btns'>
        <ul>
            <li>
                <a id="btnCreateBankClerk">
                    <p><img src="~/Areas/JJD/Content/images/05.bankManager/u1391.png" /></p>
                    <span>添加用户</span>
                </a>
            </li>
            <li class='onOff'>
                @{
                    if (Model.BankAutoAssign)
                    {
                        <p><img src="~/Areas/JJD/Content/images/05.bankManager/u1734.png" /></p>
                        <span>自动分配开启</span>
                    }
                    else
                    {
                        <p><img src="~/Areas/JJD/Content/images/05.bankManager/u1720.png" /></p>
                        <span>自动分配关闭</span>
                    }
}

            </li>
            <li>
                <a id="btnMessage">
                    <p><img src="~/Areas/JJD/Content/images/05.bankManager/u1399.png" /><b id="MessageCount">0</b></p>
                    <span>最新消息</span>
                </a>
            </li>
        </ul>
    </div>
    <div class='applys' id='applys'>
        <ul class="mui-table-view">
            <li class="mui-table-view-cell ">
                <a href="/jiajudai/bankmanager/orders?pageIndex=1&status=@Ingenious.Infrastructure.Enum.G_OrderBusinessStatusEnum.PreProcess" id="btnInProcess" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/sq1.png" />待受理<span id="InProcess">@Model.PreProcessCount</span></a>
            </li>
            <li class="mui-table-view-cell">
                <a href="/jiajudai/bankmanager/orders?pageIndex=1&status=@Ingenious.Infrastructure.Enum.G_OrderBusinessStatusEnum.PreSign" id="btnPresigned" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/sq2.png" />待签约<span id="Presigned">@Model.PreSignCount</span></a>
            </li>
            <li class="mui-table-view-cell">
                <a href="/jiajudai/bankmanager/orders?pageIndex=1&status=@Ingenious.Infrastructure.Enum.G_OrderBusinessStatusEnum.PreSuccess" id="btnPrepassed" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/sq3.png" />待放款 <span id="Prepassed">@Model.PreSuccessedCount</span></a>
            </li>
            <li class="mui-table-view-cell">
                <a href="/jiajudai/bankmanager/orders?pageIndex=1&status=@Ingenious.Infrastructure.Enum.G_OrderBusinessStatusEnum.Successed" id="btnPassed" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/sq2.png" />已放款<span id="Passed">@Model.SuccessedCount</span></a>
            </li>
            <li class="mui-table-view-cell">
                <a href="/jiajudai/bankmanager/orders?pageIndex=1&status=@Ingenious.Infrastructure.Enum.G_OrderBusinessStatusEnum.Canceled" id="btnFailed" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/sq3.png" />已失效 <span id="Failed">@Model.PassedCount</span></a>
            </li>
        </ul>
    </div>
    <div class='applys' id='sets'>
        <ul class="mui-table-view">
            <li class="mui-table-view-cell">
                <a href="../ui_07.branchManager/manageUser.html" id="btnMyClerk" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/yhgl.png" />用户管理 </a>
            </li>
            <li class="mui-table-view-cell">
                <a id="btnQA" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/bangzhu.png" />帮助与反馈</a>
            </li>
            <li class="mui-table-view-cell">
                <a href="/jiajudai/setting/index/@Model.User.Id" id="btnSettings" class="mui-navigate-right"><img src="~/Areas/JJD/Content/images/03.personalCenter/shezhi.png" />设置中心</a>
            </li>
        </ul>

    </div>

    <script src="~/Areas/JJD/Content/js/mui.js"></script>
    <script src="~/Areas/JJD/Content/js/jquery-1.11.0.min.js"></script>
    <script src="~/Areas/JJD/Content/js/BankManager/index.js"></script>
    <!--底部-->
    @Html.Action("Navbar", "Partial")
</body>

</html>