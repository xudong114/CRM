            color: #333333;
            background-color: #fff;
            border-color: #dddddd;
        }

        .client_sel {
            border-top: 2px solid #00aaef !important;
            border-left: 1px solid #e8e8e8;
            border-right: 1px solid #e8e8e8;
        }

        .client_sel_left {
            border-top: 2px solid #00aaef !important;
            border-right: 1px solid #e8e8e8;
        }

        #profile ul li {
            width: 300px;
            
            display: block;
            float: left;
        }

        #profile ul li div:first-child {
            margin-left: 10px;
            margin-top: 10px;
            color: #8fa1b2;
        }

        #profile ul li div:nth-child(2) {
            margin-left: 10px;
            font-size: 14px;
            line-height: 35px;
            width: 100%;
            height: 35px;
            padding-right: 20px;
            color: #333940;
            position: relative;
        }

        #profile ul li:hover {
            background-color: #f2f4f7;
            display: inline-block;
        }

        #profile tr {
            border-bottom: 1px dashed #e8e8e8;
        }

        #client_activity div:first-child > div {
            margin-bottom: 10px;
        }

    .ui-dialog {
        padding: 0;
        box-shadow: 0px 0px 10px 0px #41586e;
    }

    .ui-dialog-titlebar {
        height: 50px;
    }

    .ui-widget-header {
        background-color: #41586e;
        color: #fff;
        border: 0;
    }

    .ui-widget input, .ui-widget button {
        font-size: 14px !important;
    }
</style>

}

@section scripts{
<script src="~/Content/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="~/Scripts/json2.min.js"></script>
    <script src="~/Scripts/juicer-min.js"></script>
    <script>
        $().ready(function () {

            $('.nav.nav-tabs.pull-left.client_tab li').each(function (n, ele) {

                $(this).click(function () {
                    $('.nav.nav-tabs.pull-left.client_tab li').removeClass('client_sel client_sel_left');
                    if (n == 0) {
                        $(this).addClass('client_sel_left');
                    } else {
                        $(this).addClass('client_sel');
                    }
                });
            });

            $('#btnSaveActivity').click(function () {
                var clientId = '@Model.Client.Id';
                var cateId = $('#ActivityCategories').val();
                var content = $('#client_activity_content').val();
                if (content == '')
                    return;
                $.ajax({
                    type: "post",
                    dataType: "json",
                    url: "/client/CreateActivity",
                    data: { clientId: clientId, cateId: cateId, content: content },
                    success: function (data) {
                        $('#client_activity_content').val('');
                        var data = JSON.parse(data);
                        var tpl = document.getElementById('tplActivity').innerHTML;
                        var html = $(juicer(tpl, data)).hide();
                        $('.comment-list.block').prepend(html);
                        html.fadeIn(2000);
                    },
                    error: function (arg1, arg2, arg3) {
                        alert(arg3);
                    }
                });
            });

            juicer.set({
                'tag::operationOpen': '{@@',
                'tag::operationClose': '}',
                'tag::interpolateOpen': '${',
                'tag::interpolateClose': '}',
                'tag::noneencodeOpen': '$${',
                'tag::noneencodeClose': '}',
                'tag::commentOpen': '{#',
                'tag::commentClose': '}'
            });

            $('#btnNew').bind('mouseenter', function () {
                $('#btnSetting').next().hide();
                $('#btnEdit').next().hide();
                $(this).next().show();
            });
            $('#btnNew').next().bind('mouseleave', function () {
                $(this).hide();
            });

            $('#btnEdit').bind('mouseenter', function () {
                $('#btnSetting').next().hide();
                $('#btnEdit').next().hide();
                $('#btnNew').next().hide();
            });

            $('#btnSetting').bind('mouseenter', function () {
                $('#btnNew').next().hide();
                $('#btnEdit').next().hide();
                $(this).next().show();
            });

            $('#btnSetting').next().bind('mouseleave', function () {
                $(this).hide();
            });
            

            var dialog_form_clientcontact = $("#dialog-form-clientcontact").dialog({
                autoOpen: false,
                height: 600,
                width: 1000,
                modal: true,
                hide: 'explode',
                show: 'explode',
                close: function () {
                    
                }
            });

            $('#btnCreateClientContact').click(function () {
                dialog_form_clientcontact.dialog("open");
            });

            $('#formDialogClientContact').submit(function (e) {

                $.ajax({
                    type:"post",
                    url: "/client/createclientcontact",
                    data: $(this).serialize(),
                    clearForm:true,
                    success: function (data) {
                        dialog_form_clientcontact.dialog("close");
                        var data = JSON.parse(data);
                        if (data.length == 0) {
                            return;
                        }
                        var tpl = document.getElementById('tplClientContact').innerHTML;
                        var html = $(juicer(tpl, data)).hide();
                        
                        $('#tbodyContractDTOList').append(html);
                        html.fadeIn(2000);
                        $('#formDialogClientContact')[0].reset();
                    },
                    error: function (arg1, arg2, arg3) {
                        alert(arg3);

                    }
                });

                e.preventDefault();
            });

            $('#btnCloseDialogClientContact').click(function () {
                dialog_form_clientcontact.dialog("close");
            });

            $('#btnSetClientContact').click(function(){
                alert('ok');
            });


            $('#tbodyContractDTOList .xsy_su_refer_arrow').click(function () {

                $(this).parent().parent().next().toggle();

            });

            $('#tbodyContractDTOList a[act="delete"]').click(function () {
                var id = $(this).attr('code');
                $.ajax({
                    url: '/client/deleteclientcontact',
                    data: {id:id},
                    success: function (data) { $('tr[code="' + id + '"]').remove() },
                    error: function (arg1, arg2, arg3) { alert(arg3); }
                });
            });


        });
    </script>

    <script id="tplActivity" type="text/template">

        {@@each List as item,index}
        <article id="comment-id-1" class="comment-item">
            <a class="pull-left thumb-sm avatar activity_category_icon"> <img class="icon_location" /> </a> <span class="arrow left"></span>
            <section class="comment-body panel panel-default">
                <header class="panel-heading bg-white">
                    <a href="#">${item.UserName} ${item.ActivityCategoryName}</a>
                    {@@if item.CreatedDate !=  item.ModifiedDate }
                    <label class='label bg-info m-l-xs'>已编辑</label>
                    {@@/if}
                    <span class="text-muted m-l-sm pull-right"> <i class="fa fa-clock-o"></i> ${item.TimeLabel} </span>
                </header>
                <div class="panel-body">
                    <div>
                        ${item.Content}
                    </div>
                    @*<div class="comment-action m-t-sm">
                        <a href="#comment-form" class="btn btn-default btn-xs">
                            <i class="fa fa-mail-reply text-muted"></i> 评论
                        </a>
                    </div>*@
                </div>
            </section>
        </article>
        {@@/each}

    </script>

