                                                <li>
                                                    <div>所属部门</div>
                                                    <div>
                                                        @Model.Client.DepartmentFullName
                                                    </div>
                                                </li>
                                                <li>
                                                    <div>锁定状态</div>
                                                    <div></div>
                                                </li>
                                            </ul>
                                        </td>
                                    </tr>
                                </table>
                            </div>



                        </div>
                    </div>
                </section>
            </td>
            <td style="width:40%;  vertical-align:top;">
                <section class="panel panel-default" style="margin-left:10px;">
                    <header class="panel-heading text-right bg-light">
                        <ul class="nav nav-tabs pull-left client_tab_right">
                            <li class="active client_sel_left" style="width:100px;"><a href="#client_activity" data-toggle="tab">动态</a></li>
                        </ul>
                    </header>
                    <div class="panel-body">
                        <div class="tab-content">
                            <div class="tab-pane fade active in" id="client_activity">
                                <div>
                                    <div>
                                        记录类型
                                    </div>

                                    <div>
                                        @Html.DropDownList("ActivityCategories", ViewBag.ActivityCategories as SelectList, new { @class = "input-sm form-control input-s-sm inline" })
                                    </div>
                                    <div>
                                        <textarea id="client_activity_content" class="form-control" placeholder="添加跟进记录"></textarea>
                                    </div>
                                    <div class="pull-right">
                                        <a class="btn btn-default btn-sm">取消</a>
                                        <a id="btnSaveActivity" class="btn btn-primary btn-sm">确定</a>
                                    </div>
                                </div>


                                <div class="col-lg-6" style="background-color:#f7f7f7; width:100%; margin-top:20px;">
                                    <!-- .comment-list -->
                                    <section class="comment-list block" style="margin-top:10px;">
                                        @{
                                            foreach (var item in Model.ActivityDTOList.Where(item => !item.ParentId.HasValue))
                                            {
                                                <article id="comment-id-1" class="comment-item">
                                                    <a class="pull-left thumb-sm activity_category_icon" >
                                                    @{
                                                switch (item.ActivityCategory.Name)
                                                {
                                                    case "快速记录":
                                                        {
                                                            <img class="icon_note" />
                                                        } break;
                                                    case "电话":
                                                        {
                                                            <img class="icon_call" />
                                                        } break;
                                                    case "拜访签到":
                                                        {
                                                            <img class="icon_location" />
                                                        } break;
                                                }

}
                                                    </a> <span class="arrow left"></span>
                                                    <section class="comment-body panel panel-default">
                                                        <header class="panel-heading bg-white">
                                                            <a href="#">
                                                                @item.UserName
                                                                @if (!string.IsNullOrWhiteSpace(item.ActivityCategoryName))
                                                                {
                                                                    @Html.Raw(string.Format("[{0}]", item.ActivityCategoryName))
                                                                }
                                                            </a>
                                                            @if (item.CreatedDate != item.ModifiedDate)
                                                            {
                                                                @Html.Raw("<label class='label bg-info m-l-xs'>已编辑</label>")
                                                            }

                                                            <span class="text-muted m-l-sm pull-right"> <i class="fa fa-clock-o"></i> @item.TimeLabel </span>
                                                        </header>
                                                        <div class="panel-body">
                                                            <div>
                                                                @Html.Raw(item.Content)
                                                            </div>
                                                            @*<div class="comment-action m-t-sm">
                                                                <a href="#comment-form" class="btn btn-default btn-xs">
                                                                    <i class="fa fa-mail-reply text-muted"></i> 评论
                                                                </a>
                                                            </div>*@
                                                        </div>
                                                    </section>
                                                </article>
                                            }

                                        }


                                    </section>
                                    <!-- / .comment-list -->
                                </div>


                            </div>
                        </div>
                    </div>
                </section>
            </td>
        </tr>
    </table>
</div>



