    <style>
        .panel-heading {
            height: 40px;
        }

        #client-content {
            top: 10px;
            position:relative;
        }

        .client_tab {
        }

        .client_tab li, .client_tab_right li {
            border-top-width: 2px;
            border-top-style: solid;
            border-top-color: #fff;
            text-align: center;
        }

        .panel {
            border: 0px;
        }

